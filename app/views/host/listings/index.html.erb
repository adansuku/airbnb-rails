<h1>Your listings</h1>

<%@listings.each do |l|%>
   <%= link_to l.title, host_listings_path %>
   <%= l.status%>
   <%= link_to "Edit", edit_host_listing_path(l)%>
   <%= button_to "Delete", host_listing_path(l), method: :delete %>

<% end %>
<p>Find me in app/views/host/listings/index.html.erb</p>
